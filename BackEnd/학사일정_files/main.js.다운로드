  function createFullpage() {
	if (fullPageCreated === false) {
	  fullPageCreated = true;
	  $(".main #_contentBuilder").fullpage({
		navigation: false,
		verticalCentered: true,
		keyboardScrolling: true,
		animateAnchor: true,
		recordHistory: true,
		anchors: ['page1', 'page2', 'page3', 'page4'],
		responsiveHeight: 860,
		responsiveWidth: 1300,
		onLeave: function (index, nextIndex, direction) {
		  if (nextIndex == 1) {
			$('.head-navi').removeClass('white');
			$('.wrap-pager button').removeClass('on');
			$('.wrap-pager button.pager1').addClass('on');
		  } else if (nextIndex == 2) {
			$('.head-navi').addClass('white');
			$('.wrap-pager button').removeClass('on');
			$('.wrap-pager button.pager2').addClass('on');
		  } else if (nextIndex == 3) {
			$('.head-navi').removeClass('white');
			$('.wrap-pager button').removeClass('on');
			$('.wrap-pager button.pager3').addClass('on');
		  } else if (nextIndex == 4) {
			$('.head-navi').addClass('white');
			$('.wrap-pager button').removeClass('on');
			$('.wrap-pager button.pager4').addClass('on');
		  } else if (nextIndex == 5) {
			$('.head-navi').removeClass('white');
			$('.wrap-pager button').removeClass('on');
		  } else if (nextIndex == 6) {
			$('.head-navi').removeClass('white');
			$('.wrap-pager button').removeClass('on');
		  }
		}
	  });
	}
  }

  //Height limit
  $(window).load(function () {
	//Height 1200px ~ Destory
	if ($(window).height() >= 1200) {
	  if ($('body').hasClass('main')) {
		fullPageCreated = false;
		$.fn.fullpage.destroy('all');
	  }
	} else {
	  if (nHref.indexOf('contentsHtmlView.do') > -1) {
	  } else {
		if ($('body').hasClass('main')) {
		  //접근성 임시조치
		  createFullpage();
		};
	  }
	}
  });

