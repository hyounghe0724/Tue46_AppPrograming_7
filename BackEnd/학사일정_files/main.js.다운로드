	  closePlan.on('click',function(){
		$('.sche-alb .sche-comt td a.focus').focus();
	  })
	},500);
  });

  //접근성 인물소개 닫기 버튼 title 추가
  $('.profile .detail-info .title .btn-close-detail').attr('title','교수소개 닫기');

  //접근성 대학에 위치,전화번호에 문구 추가
  $('.wrap-depart .desc .contact .location').prepend('<span class="hidden">위치</span>');
  $('.wrap-depart .desc .contact .tel').prepend('<span class="hidden">전화번호</span>');

  //접근성 학사일정 탭 건너뛰기
  $('#menu160_obj5987').find('button').attr('tabindex', '-1');
  $('#menu160_obj5987').find('a').attr('tabindex', '-1');

  //접근성 조치
