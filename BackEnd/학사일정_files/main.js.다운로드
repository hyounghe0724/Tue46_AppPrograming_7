  $('.sche-alb .sche-comt td a').on('click',function(){
	$('.sche-alb .sche-comt td a').removeClass('focus');
	$(this).addClass('focus');
	setTimeout(function(){
	  var closePlan = $('#layer_planner .btn_close');
